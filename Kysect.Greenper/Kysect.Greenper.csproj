    <PackageReference Include="Kysect.CentumFramework.Utility" Version="1.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="6.0.0-preview.7.21377.19" />
  </ItemGroup>

</Project>
